{
    "version": "0.2.0",
    "configurations": [
    {
        "name": "Dev",
        "type": "node",
        "request": "launch",
        "runtimeArgs": [
            "run",
            "dev"
        ],
        "runtimeExecutable": "npm",
        "skipFiles": [
            "<node_internals>/**"
        ]        
    },
        {
            "name": "Jest",
            "type": "node",
            "request": "launch",
            "runtimeArgs": [
                "--inspect-brk",
                "${workspaceRoot}/node_modules/jest/bin/jest.js",
                "--runInBand"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}